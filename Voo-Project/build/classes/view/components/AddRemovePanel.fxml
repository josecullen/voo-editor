<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<fx:root type="AnchorPane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
   <children>
      <HBox prefHeight="31.0" prefWidth="232.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TextField fx:id="txtNewEntity" prefHeight="30.0" prefWidth="113.0" HBox.hgrow="ALWAYS">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <Button fx:id="btnAddEntity" mnemonicParsing="false" text="+">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btnRemoveEntity" mnemonicParsing="false" prefWidth="30.0" text="-">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btnEditEntity" mnemonicParsing="false" prefHeight="30.0" prefWidth="44.0" text="Edit" HBox.hgrow="ALWAYS">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</fx:root>
