                     <children>
                        <TableView fx:id="mdoTable" editable="true" prefHeight="175.0" prefWidth="381.0" VBox.vgrow="ALWAYS">
					              <columns>
					              	<TableColumn fx:id="mdoNombreColumn" editable="false" prefWidth="75.0" text="Mano de Obra" />
					                <TableColumn fx:id="mdoCostoHoraColumn" editable="false" prefWidth="75.0" text="Costo/hora" />
					                <TableColumn fx:id="mdoCantidadHorasColumn" editable="true" prefWidth="75.0" text="Cantidad/horas" />
					                <TableColumn fx:id="mdoValorColumn" editable="false" prefWidth="75.0" text="Valor" />
					              </columns>
					               <columnResizePolicy>
					                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
					               </columnResizePolicy>
					            </TableView>

                        <HBox VBox.vgrow="SOMETIMES">
                           <children>
                              <ComboBox fx:id="cbMDO" prefHeight="25.0" prefWidth="315.0" promptText="Mano de Obra" HBox.hgrow="ALWAYS" />
<!--                               <Button fx:id="btnAddMDO" mnemonicParsing="false" onAction="#addMDOEventHandler" prefWidth="25.0" text="+" /> -->
<!--                               <Button fx:id="btnRemoveMDO" mnemonicParsing="false" onAction="#removeMDOEventHandler" prefWidth="25.0" text="-" /> -->
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </content>
