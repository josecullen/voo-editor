                              <TableColumn fx:id="maquinaCantidadMinutosColumn1" editable="true" prefWidth="75.0" text="Cantidad/minutos" />
                              <TableColumn fx:id="maquinaValorColumn1" editable="false" prefWidth="75.0" text="Valor" />
                           </columns>
                        </TableView>
                        <HBox prefWidth="381.0" VBox.vgrow="SOMETIMES">
                           <children>
                              <ComboBox fx:id="cbMaquina1" prefWidth="332.0" HBox.hgrow="SOMETIMES" />
<!--                               <Button fx:id="btnAddMaquina1" mnemonicParsing="false" onAction="#addMaquinaEventHandler" prefWidth="25.0" text="+" /> -->
<!--                               <Button fx:id="btnRemoveMaquina1" mnemonicParsing="false" onAction="#removeMaquinaEventHandler" prefWidth="25.0" text="-" /> -->
                           </children>
